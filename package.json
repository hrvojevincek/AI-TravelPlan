{
  "name": "travelplan",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "ts-node prisma/seed.js"
  },
  "scripts": {
    "predev": "docker-compose up -d",
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint --ext .js,.ts .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "seed": "prisma db seed",
    "dev:test": "jest --watch",
    "test:docker:up": "docker compose -f docker-compose.yml -f docker-compose.test.yml up -d",
    "test:docker:down": "docker stop db_test && docker container rm db_test",
    "predev:test": "yarn test:docker:up && yarn prisma migrate deploy",
    "pretest": "yarn prisma migrate deploy",
    "test": "jest --ci --coverage",
    "postdev:test": "yarn test:docker:down"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@prisma/client": "^4.16.2",
    "@react-google-maps/api": "^2.18.1",
    "@types/node": "20.3.1",
    "@types/react": "18.2.13",
    "@types/react-dom": "18.2.6",
    "antd": "^5.7.0",
    "autoprefixer": "10.4.14",
    "cloudinary-react": "^1.8.1",
    "dotenv": "^16.3.1",
    "eslint-config-next": "13.4.7",
    "next": "13.4.7",
    "next-auth": "^4.22.1",
    "openai": "^3.3.0",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intersection-observer": "^9.5.2",
    "react-router-dom": "^6.14.0",
    "react-youtube": "^10.1.0",
    "tailwindcss": "3.3.2",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^2.8.8",
    "prisma": "^4.16.2",
    "ts-node": "^10.9.1"
  },
  "resolutions": {
    "cross-fetch": "4.0.0"
  }
}
